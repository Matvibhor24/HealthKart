<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid android:color="#FF03BEC0"/>
            <corners android:radius="25dp" />
            <stroke
                android:width="2px"
                android:color="#959597" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#EBF9FF"/>
            <corners android:radius="25dp" />
            <stroke
                android:width="2px"
                android:color="#959597" />
        </shape>
    </item>
</selector>
